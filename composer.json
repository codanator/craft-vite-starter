{
  "name": "codanator/craft-vite-starter",
  "description": "True Craft CMS with DDEV & Vite",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "keywords": [
    "true-source",
    "craft",
    "cms",
    "craftcms",
    "vite"
  ],
  "license": "0BSD",
  "homepage": "https://github.com/codanator/craft-vite-starter",
  "type": "project",
  "support": {
    "issues": "https://github.com/codanator/craft-vite-starter/issues",
    "forum": "https://craftcms.stackexchange.com/",
    "source": "https://github.com/codanator/craft-vite-starter",
    "docs": "https://github.com/codanator/craft-vite-starter#readme",
    "rss": "https://craftcms.com/changelog.rss"
  },
  "require": {
    "craftcms/cms": "^5.0.2",
    "nystudio107/craft-vite": "^4.0.9",
    "vlucas/phpdotenv": "^5.5.0"
  },
  "require-dev": {
    "craftcms/generator": "^2.0.1",
    "yiisoft/yii2-shell": "^2.0.5"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "allow-plugins": {
      "craftcms/plugin-installer": true,
      "yiisoft/yii2-composer": true
    },
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "8.2.0"
    }
  }
}
